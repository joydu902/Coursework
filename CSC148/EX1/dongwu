class Animal:
    def __init__(self):
        self.x = 1
        self.y = 1
        self.z = 1

    def move_forward(self):
        self.z += 1
        super().move_forward()

    def fly_up(self):
        self.y += 1

    def fly_down(self):
        self.y -= 1

class flyinganimal(Animal):
    def fly_up(self):
        self.y += 1

    def fly_down(self):
        self.y -= 1


